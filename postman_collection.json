{
  "info": {
    "name": "BlogAPI"
  },
  "item": [
    {
      "name": "auth",
      "item": [
        {
          "name": "register",
          "item": [
            {
              "name": "Register",
              "request": {
                "method": "POST",
                "url": "{{baseUrl}}/auth/register",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  },
                  {
                    "key": "Accept",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"username\": \"<string>\",\n  \"password\": \"<string>\"\n}"
                }
              }
            }
          ]
        },
        {
          "name": "login",
          "item": [
            {
              "name": "Login",
              "request": {
                "method": "POST",
                "url": "{{baseUrl}}/auth/login",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  },
                  {
                    "key": "Accept",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"username\": \"<string>\",\n  \"password\": \"<string>\"\n}"
                }
              }
            }
          ]
        },
        {
          "name": "revoke",
          "item": [
            {
              "name": "Revoke Token",
              "request": {
                "method": "POST",
                "url": "{{baseUrl}}/auth/revoke",
                "header": [
                  {
                    "key": "Accept",
                    "value": "application/json"
                  }
                ]
              }
            }
          ]
        },
        {
          "name": "logout",
          "item": [
            {
              "name": "Logout",
              "request": {
                "method": "POST",
                "url": "{{baseUrl}}/auth/logout",
                "header": [
                  {
                    "key": "Accept",
                    "value": "application/json"
                  }
                ],
                "auth": {
                  "type": "bearer",
                  "bearer": [
                    {
                      "key": "token",
                      "value": "{{bearerToken}}"
                    }
                  ]
                }
              }
            }
          ]
        }
      ]
    },
    {
      "name": "api",
      "item": [
        {
          "name": "blog",
          "item": [
            {
              "name": "search",
              "item": [
                {
                  "name": "Search Blog",
                  "request": {
                    "method": "GET",
                    "url": "{{baseUrl}}/api/blog/search?q=<string>&sort=asc",
                    "header": [
                      {
                        "key": "Accept",
                        "value": "application/json"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "name": "create",
              "item": [
                {
                  "name": "Create Blog",
                  "request": {
                    "method": "POST",
                    "url": "{{baseUrl}}/api/blog/create",
                    "header": [
                      {
                        "key": "Content-Type",
                        "value": "application/json"
                      },
                      {
                        "key": "Accept",
                        "value": "application/json"
                      }
                    ],
                    "body": {
                      "mode": "raw",
                      "raw": "{\n  \"title\": \"<string>\",\n  \"content\": \"<string>\"\n}"
                    },
                    "auth": {
                      "type": "bearer",
                      "bearer": [
                        {
                          "key": "token",
                          "value": "{{bearerToken}}"
                        }
                      ]
                    }
                  }
                }
              ]
            },
            {
              "name": "{blog_id}",
              "item": [
                {
                  "name": "Get Blog",
                  "request": {
                    "method": "GET",
                    "url": "{{baseUrl}}/api/blog/:blog_id",
                    "header": [
                      {
                        "key": "Accept",
                        "value": "application/json"
                      }
                    ]
                  }
                },
                {
                  "name": "Update Blog",
                  "request": {
                    "method": "PUT",
                    "url": "{{baseUrl}}/api/blog/:blog_id",
                    "header": [
                      {
                        "key": "Content-Type",
                        "value": "application/json"
                      },
                      {
                        "key": "Accept",
                        "value": "application/json"
                      }
                    ],
                    "body": {
                      "mode": "raw",
                      "raw": "{\n  \"title\": \"<string>\",\n  \"content\": \"<string>\"\n}"
                    },
                    "auth": {
                      "type": "bearer",
                      "bearer": [
                        {
                          "key": "token",
                          "value": "{{bearerToken}}"
                        }
                      ]
                    }
                  }
                },
                {
                  "name": "Delete Blog",
                  "request": {
                    "method": "DELETE",
                    "url": "{{baseUrl}}/api/blog/:blog_id",
                    "header": [
                      {
                        "key": "Accept",
                        "value": "application/json"
                      }
                    ],
                    "auth": {
                      "type": "bearer",
                      "bearer": [
                        {
                          "key": "token",
                          "value": "{{bearerToken}}"
                        }
                      ]
                    }
                  }
                }
              ]
            },
            {
              "name": "upload",
              "item": [
                {
                  "name": "Upload Image",
                  "request": {
                    "method": "POST",
                    "url": "{{baseUrl}}/api/blog/upload",
                    "header": [
                      {
                        "key": "Content-Type",
                        "value": "multipart/form-data"
                      },
                      {
                        "key": "Accept",
                        "value": "application/json"
                      }
                    ],
                    "body": {
                      "mode": "formdata",
                      "formdata": [
                        {
                          "key": "file",
                          "type": "file"
                        }
                      ]
                    },
                    "auth": {
                      "type": "bearer",
                      "bearer": [
                        {
                          "key": "token",
                          "value": "{{bearerToken}}"
                        }
                      ]
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://127.0.0.1:8000"
    },
    {
      "key": "bearerToken",
      "value": ""
    }
  ]
}